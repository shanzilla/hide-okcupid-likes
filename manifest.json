{
  "manifest_version": 2,

  "name": "Hide OkCupid Likes",
  "version": "1.0",
  "description": "This extension allows users to customize the display of OkCupid likes.",
  "options_page": "options.html",

  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "js": [ "content.js" ],
      "css": [ "style.css" ],
      "matches": [ "https://okcupid.com/*", "https://www.okcupid.com/*" ]
    }
  ]
}
